<% if data.present? %>
  <h2>Uploaded Data</h2>
  <table border="1">
    <thead>
      <tr>
        <% data.first.keys.each do |key| %>
          <th><%= key %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% data.each do |row| %>
        <tr>
          <% row.values.each do |value| %>
            <td><%= value %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
